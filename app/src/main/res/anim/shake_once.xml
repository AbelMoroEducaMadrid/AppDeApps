        android:interpolator="@android:anim/accelerate_decelerate_interpolator" />

    <translate
        android:duration="50"
        android:fromXDelta="10%"
        android:toXDelta="-8%"
        android:startOffset="100"
        android:interpolator="@android:anim/accelerate_decelerate_interpolator" />

    <translate
        android:duration="50"
        android:fromXDelta="-8%"
        android:toXDelta="8%"
