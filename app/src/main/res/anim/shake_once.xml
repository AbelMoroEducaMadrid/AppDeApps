        android:interpolator="@android:anim/accelerate_decelerate_interpolator" />

    <translate
        android:duration="50"
        android:fromXDelta="8%"
        android:toXDelta="0%"
        android:startOffset="200"
        android:interpolator="@android:anim/accelerate_decelerate_interpolator" />
</set>
